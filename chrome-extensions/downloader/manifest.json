{
  "manifest_version": 2,
  "version": "1.0.0",
  "minimum_chrome_version": "31",

  "name": "Download Selected Links",
  "description": "Select links on a page and download them.",

  "permissions": [
    "downloads",
     "<all_urls>"
   ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
